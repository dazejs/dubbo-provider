{
  "name": "@dazejs/dubbo-provider",
  "version": "0.1.0",
  "description": "dubbo service for dazejs",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env DAZE_ENV=dev DEBUG=dubbo-provider:* nodemon --signal SIGTERM --exec ts-node example/index.ts --source-map --preserve-symlinks",
    "build": "tsc",
    "watch": "tsc -w",
    "prod": "cross-env DAZE_ENV=prod node dist/index.js",
    "test": "jest --verbose --runInBand --forceExit",
    "test:coverage": "jest --runInBand --forceExit --coverage",
    "commitlint": "commitlint -e $GIT_PARAMS",
    "prepublishOnly": "npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=10.15.0"
  },
  "peerDependencies": {
    "@dazejs/framework": ">=2.6.0",
    "reflect-metadata": ">=0.1.12"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@dazejs/framework": "2.7.1",
    "@types/compare-versions": "3.3.0",
    "@types/debug": "4.1.5",
    "@types/jest": "25.1.2",
    "@types/js-to-java": "2.4.0",
    "@types/node": "13.9.2",
    "@types/node-zookeeper-client": "0.2.7",
    "@types/supertest": "2.0.8",
    "@typescript-eslint/eslint-plugin": "2.28.0",
    "@typescript-eslint/parser": "2.24.0",
    "commitlint": "8.3.5",
    "cross-env": "7.0.2",
    "eslint": "6.8.0",
    "husky": "4.2.3",
    "jest": "25.3.0",
    "nodemon": "2.0.2",
    "reflect-metadata": "0.1.13",
    "supertest": "4.0.2",
    "ts-jest": "25.4.0",
    "ts-node": "8.7.0",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "compare-versions": "3.6.0",
    "debug": "4.1.1",
    "hessian.js": "2.9.0",
    "js-to-java": "2.6.1",
    "node-zookeeper-client": "1.1.0"
  }
}
