{
  "name": "@dazejs/dubbo-provider",
  "version": "0.0.1",
  "description": "dubbo service for dazejs",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "cross-env DAZE_ENV=dev DEBUG=daze-framework:* nodemon --signal SIGTERM --exec ts-node src/index.ts --source-map --preserve-symlinks",
    "build": "tsc",
    "watch": "tsc -w",
    "prod": "cross-env DAZE_ENV=prod node dist/index.js",
    "example": "cross-env DAZE_ENV=prod ts-node example/index.ts"
  },
  "engines": {
    "node": ">=10.15.0"
  },
  "peerDependencies": {
    "@dazejs/framework": ">=2.2.1"
  },
  "devDependencies": {
    "@dazejs/framework": "^2.2.1",
    "@types/js-to-java": "^2.4.0",
    "@types/node": "^12.7.5",
    "@types/node-zookeeper-client": "^0.2.6",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.2.0",
    "nodemon": "^1.18.3",
    "ts-node": "^8.4.1",
    "typescript": "^3.7.4"
  },
  "dependencies": {
    "hessian.js": "^2.9.0",
    "js-to-java": "^2.6.1",
    "node-zookeeper-client": "^1.1.0",
    "reflect-metadata": "^0.1.13"
  }
}
